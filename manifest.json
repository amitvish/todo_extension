{
    "manifest_version": 3,
    "name": "ToDo List",
    "version": "1.0",
    "description": "A simple to-do list Chrome extension.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      }
    }
  }
  